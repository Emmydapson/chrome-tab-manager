{
    "manifest_version": 3,
    "name": "Smart Tab Manager",
    "description": "An intelligent Chrome extension for managing and organizing tabs.",
    "version": "1.0",
    "permissions": ["tabs", "storage" ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "host_permissions": [
    "<all_urls>"
  ]
  }
  